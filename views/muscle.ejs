<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>筋トレ種目一覧</title>
</head>
<body>
  <h2>筋トレ種目リスト</h2>
  <table>
    <tr><th>ID</th><th>種目名</th><th>部位</th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr>
          <td><%= row.id %></td>
          <td><a href="/muscle/<%= number %>"><%= row.name %></a></td>
          <td><%= row.part %></td>
        </tr>
        <% number += 1 %>
      <% } %>
  </table>
  <a href="/muscle/create">新規種目を追加</a>
</body>
</html>